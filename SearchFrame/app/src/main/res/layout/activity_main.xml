<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button_query_link_man"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/query_link_man" />

    <Button
        android:id="@+id/button_query_call_history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/query_call_history"
        app:layout_constraintTop_toBottomOf="@id/button_query_link_man" />

</android.support.constraint.ConstraintLayout>